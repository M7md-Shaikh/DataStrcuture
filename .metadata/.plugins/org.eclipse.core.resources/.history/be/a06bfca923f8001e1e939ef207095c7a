package application;

public class NodeDS {
    private Location data;
    private NodeDS next;
    private NodeSS firstMartyr;

    public NodeDS(Location data) {
        this.data = data;
    }

    public Location getData() {
        return data;
    }

    public void setData(Location data) {
        this.data = data;
    }

    public NodeDS getNext() {
        return next;
    }

    public void setNext(NodeDS next) {
        this.next = next;
    }

    public NodeSS getFirstMartyr() {
        return firstMartyr;
    }

    public void setFirstMartyr(NodeSS firstMartyr) {
        this.firstMartyr = firstMartyr;
    }
}
